{
  "name": "main",
  "version-string": "latest",
  "dependencies": [
    "doctest",
    "spdlog",
    "gap"
  ],
  "default-features": [
    "cxx-common-llvm-14"
  ],
  "features": {
    "system-remill": {
      "description": "Build with system remill",
      "dependencies": [
        {
          "name": "xed",
          "default-features": false
        },
        {
          "name": "glog",
          "default-features": false
        },
        {
          "name": "z3",
          "default-features": false
        }
      ]
    },
    "system-llvm": {
      "description": "Build with system llvm",
      "dependencies": [
        {
          "name": "remill",
          "default-features": false,
          "features": ["system-llvm"]
        }
      ]
    },
    "cxx-common-llvm-14": {
      "description": "Build with cxx-common llvm-14 port",
      "dependencies": [
        {
          "name": "remill",
          "default-features": false,
          "features": ["cxx-common-llvm-14"]
        }
      ]
    }
  }
}
